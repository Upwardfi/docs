<td><code>product_id</code> <em>optional</em></td>
<td>string</td>
<td>query</td>
<td>Product ID</td>
</tr>
<tr>
<td><code>status</code> <em>optional</em></td>
<td>string</td>
<td>query</td>
<td>Status of bill payment</td>
</tr>
<tr>
<td><code>start_date</code> <em>optional</em></td>
<td>string</td>
<td>query</td>
<td>Paid at since date</td>
</tr>
<tr>
<td><code>end_date</code> <em>optional</em></td>
<td>string</td>
<td>query</td>
<td>Paid at until date</td>
</tr>
